
<%= render 'pages/level1_nav' %>
<h1>Index</h1>
<table cellpadding="5">
    <tr>
        <th>id</th>
        <th>last_name</th>
        <th>created_at</th>
        <th>updated_at</th>
    </tr>
    <tr>
        <% @users.each do |user| %>
            <td><%= user.id %></td>
            <td><%= link_to user.last_name, user_path(user) %></td>
            <td><%= user.created_at %></td>
            <td><%= user.updated_at %></td>
        <%end %>
    </tr>
</table>