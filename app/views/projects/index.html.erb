<h1>Index</h1>
<table cellpadding="5">
    <tr>
        <th>id</th>
        <th>name</th>
        <th>created_at</th>
        <th>updated_at</th>
    </tr>
    <tr>
        <% @projects.each do |project| %>
            <td><%= project.id %></td>
            <td><%= project.name %></td>
            <td><%= project.created_at %></td>
            <td><%= project.updated_at %></td>
        <%end %>
    </tr>
</table>