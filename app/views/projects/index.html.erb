<%= render 'pages/level1_nav' %>
<h1>Projects</h1>
<%= render 'index_nav' %>
<table cellpadding="5">
    <tr>
        <th>delete?</th>
        <th>id</th>
        <th>name</th>
        <th>created_at</th>
        <th>updated_at</th>
    </tr>
    <% @projects.each do |project| %>
        <tr>
            <td><%= button_to "Delete", Project.find(project.id), :method => :delete %></td>
            <td><%= project.id %></td>
            <td><%= link_to project.name, project_path(project) %></td>
            <td><%= project.created_at %></td>
            <td><%= project.updated_at %></td>
        </tr>
    <%end %>
</table>